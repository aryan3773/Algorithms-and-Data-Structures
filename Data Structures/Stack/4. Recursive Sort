def sortedInsert(s,element):
    if len(s)==0 or element>s[-1]:
        s.append(element)
        return
    else:
        temp=s.pop()
        sortedInsert(s,element)
        s.append(temp)

def sortStack(s):
    if len(s)!=0:
        temp=s.pop()
        sortStack(s)
        sortedInsert(s,temp)

def printStack(s):
    for i in range(len(s)-1,-1,-1):
        print(s[i],end=" ")
    print()
    
s=[]
s.append(30)
s.append(-5)
s.append(14)
s.append(18)
s.append(12)
s.append(-3)


print("Original")
printStack(s)

sortStack(s)

print("Sorted Stack")
printStack(s)
