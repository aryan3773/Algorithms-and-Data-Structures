def nge(l):
    for i in range(len(l)):
        ans=-1
        for j in range(i+1,len(l)):
            if l[i]<l[j]:
                ans=l[j]
                break
        print(str(l[i])+" -> "+str(ans))

def createStack():
    stack=[]
    return stack

def isEmpty(stack):
    f=False
    if len(stack)==0:
        f=True
    return f

def push(stack,x):
    stack.append(x)

def pop(stack):
    if isEmpty(stack):
        print("Error")
    else:
        return stack.pop()
    
def stack_nge(x):
    s=createStack()
    element=0
    next=0
    
    push(s,x[0])
    for i in range(1,len(x)):
        next=x[i]
        if (isEmpty(s)==False):
            element=pop(s)
            while(element<next):
                 print(str(element)+" -> "+str(next))
                 if isEmpty(s):
                     break
                 element=pop(s)
            if element>next:
                push(s,element)
                
            
        push(s,next)
    while(isEmpty(s)==False):
        element=pop(s)
        next=-1
        print(str(element)+" -> "+str(next))

p=[5,18,16,13,4,12,3,17,6]

import time

start=time.time()
stack_nge(p)
end=time.time()
print(end - start)

start=time.time()
nge(p)
end=time.time()
print(end - start)
