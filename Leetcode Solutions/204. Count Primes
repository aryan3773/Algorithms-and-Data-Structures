def sieve_prime(x):
    p=[1]*(x+1)
    p[0]=0
    p[1]=0
    i=2
    while(i*i<=x):
        if p[i]==1:
            for j in range(i*i,x+1,i):
                p[j]=0
        i+=1
    return p

class Solution:
    def countPrimes(self,n):
        if n==0 or n==1:
            return 0
        else:
            s=sieve_prime(n)
            s=s[:n]
            return s.count(1)
